class User {
    constructor(id, name) {
        this.id = id
        this.name = name
    }

    hasAccess() {
        this.name === 'Bob'
    }
}

const users = {
    new User(1, 'Bob')
    new User(2, 'Jhon')
}

function getUser(id) {
    return users.find(user => user.id === id)
}

function printUser(id) {
    const user = getUser(id)
}
